<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.missioncontrol.server</string>
    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/node</string>
        <string>--enable-source-maps</string>
        <string>--max-old-space-size=256</string>
